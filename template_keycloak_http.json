                    {
                        "uuid": "f324819fe58e4049981f3f609252b3bf",
                        "name": "Login Error Ratio %",
                        "type": "CALCULATED",
                        "key": "keycloak.login.error_ratio",
                        "value_type": "FLOAT",
                        "units": "%",
                        "params": "100 * ( last(//keycloak.login.errors.rate) / ( last(//keycloak.login.requests.rate) + (last(//keycloak.login.requests.rate)=0) ) )",
                        "description": "This performs the math: (Errors / Total) * 100.\n\n + (...=0) prevents a \"Division by Zero\" error in Zabbix if there is absolutely no traffic on the login page.\n\nRef: https://www.keycloak.org/observability/keycloak-service-level-indicators",
                        "tags": [
                            {
                                "tag": "component",
                                "value": "http"
                            }
                        ],
                        "triggers": [
                            {
                                "uuid": "6faa231338064b2d80fea2d7899c69e8",
                                "expression": "min(/Template App Keycloak by HTTP/keycloak.login.error_ratio, 5m) > 5",
                                "name": "High Login Error Rate (>5%)",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
                                "description": "The ratio of 500/Server Errors on login pages has been above 5% for the last 5 minutes.",
                                "manual_close": "YES"
                            }
                        ]
                    },
