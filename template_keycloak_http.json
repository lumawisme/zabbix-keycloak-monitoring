                        "description": "This counts only the failed requests to login pages.\n\n\nRef: https://www.keycloak.org/observability/keycloak-service-level-indicators",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "http_server_requests_seconds_count{uri=~\"/realms/.*/protocol/.*|/realms/.*/login-actions.*\", outcome=\"SERVER_ERROR\"}",
