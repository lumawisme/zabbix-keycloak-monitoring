                        ],
                        "triggers": [
                            {
                                "uuid": "d8a8515b646c49fa91197589aedacb7d",
                                "expression": "min(/Template App Keycloak by HTTP/keycloak.login.response_time.max, 5m) > 0.25",
                                "name": "Login Response Time is High (>250ms)",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
