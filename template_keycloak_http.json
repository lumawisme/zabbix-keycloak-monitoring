                        "value_type": "FLOAT",
                        "units": "rps",
                        "description": "This counts all requests to login pages.\n\nRef: https://www.keycloak.org/observability/keycloak-service-level-indicators",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "http_server_requests_seconds_count{uri=~\"/realms/.*/protocol/.*|/realms/.*/login-actions.*\"}",
                                    "function",
                                    "sum"
