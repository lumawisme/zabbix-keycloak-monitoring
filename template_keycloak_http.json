                    {
                        "uuid": "44d4b5f0485a47058d88d6758af0863c",
                        "name": "Max Login Response Time",
                        "type": "DEPENDENT",
                        "key": "keycloak.login.response_time.max",
                        "value_type": "FLOAT",
                        "units": "s",
                        "description": "This metric tracks the single slowest request in the last interval).\n\n\nRef: https://www.keycloak.org/observability/keycloak-service-level-indicators",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "http_server_requests_seconds_max{uri=~\"/realms/.*/protocol/.*|/realms/.*/login-actions.*\"}",
                                    "function",
                                    "max"
                                ],
                                "error_handler": "CUSTOM_VALUE",
                                "error_handler_params": "0"
                            }
                        ],
                        "master_item": {
                            "key": "keycloak.get_metrics"
                        },
