                                    "agroal_active_count",
                                    "value",
                                    ""
                                ]
                            }
                        ],
                        "master_item": {
                            "key": "keycloak.get_metrics"
                        },
                        "tags": [
                            {
                                "tag": "component",
                                "value": "database"
                            }
                        ]
                    },
                    {
                        "uuid": "278f241b632740678b12b8fc90428333",
                        "name": "DB Available Connections",
                        "type": "DEPENDENT",
                        "key": "keycloak.db.available",
                        "value_type": "FLOAT",
                        "description": "Number of idle connections in the pool, available to be acquired.",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "agroal_available_count",
                                    "value",
                                    ""
                                ]
                            }
                        ],
                        "master_item": {
                            "key": "keycloak.get_metrics"
                        },
                        "tags": [
                            {
                                "tag": "component",
                                "value": "database"
                            }
                        ]
                    },
                    {
                        "uuid": "04f254800a0643e095f6c56865018801",
                        "name": "DB Threads Waiting",
                        "type": "DEPENDENT",
                        "key": "keycloak.db.awaiting",
                        "value_type": "FLOAT",
                        "description": "How many threads are blocked waiting for a database connection. This should be 0.",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "agroal_awaiting_count",
                                    "value",
                                    ""
                                ]
                            }
                        ],
                        "master_item": {
                            "key": "keycloak.get_metrics"
                        },
                        "tags": [
                            {
                                "tag": "component",
                                "value": "database"
                            }
                        ],
                        "triggers": [
                            {
                                "uuid": "0733f5235b964b04837f28f8c4eb5aa5",
                                "expression": "min(/Template App Keycloak by HTTP/keycloak.db.awaiting, 5m) > 0",
                                "name": "Database Threads are Waiting",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
                                "description": "Threads are queued up waiting for a database connection. This causes immediate slowness for users.",
                                "manual_close": "YES"
                            }
                        ]
                    },
                    {
                        "uuid": "6f0e8d1ecc8e47ca8445bcb8a340f740",
                        "name": "Max DB Blocking Time",
                        "type": "DEPENDENT",
                        "key": "keycloak.db.block_time_max",
                        "value_type": "FLOAT",
                        "units": "ms",
                        "description": "The longest time a thread had to wait for a DB connection. High spikes indicate DB slowness.",
                        "preprocessing": [
                            {
                                "type": "PROMETHEUS_PATTERN",
                                "parameters": [
                                    "agroal_blocking_time_max_milliseconds",
                                    "value",
                                    ""
                                ]
                            }
                        ],
                        "master_item": {
                            "key": "keycloak.get_metrics"
                        },
                        "tags": [
                            {
                                "tag": "component",
                                "value": "database"
                            }
                        ],
                        "triggers": [
                            {
                                "uuid": "2f7e70eeb35c4ce5bfe4dd48972532cb",
                                "expression": "min(/Template App Keycloak by HTTP/keycloak.db.block_time_max, 5m) > 1000",
                                "name": "High Database Blocking Time",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
                                "description": "It is taking too long to get a connection",
                                "manual_close": "YES"
                            }
                        ]
                    },
                    {
                        "uuid": "6d89f31e70ab4c44bb9ee7d128501471",
                        "name": "DB Pool Utilization",
                        "type": "CALCULATED",
                        "key": "keycloak.db.utilization",
                        "delay": "5m",
                        "value_type": "FLOAT",
                        "units": "%",
                        "params": "100 * last(//keycloak.db.active_count) / ( last(//keycloak.db.active_count) + last(//keycloak.db.available) )",
                        "tags": [
                            {
                                "tag": "component",
                                "value": "database"
                            }
                        ],
                        "triggers": [
                            {
                                "uuid": "fa81533c28c64a879c0aefbd9cc4ca4e",
                                "expression": "min(/Template App Keycloak by HTTP/keycloak.db.utilization, 5m) > 90",
                                "name": "Database Connection Pool is almost full",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
                                "description": "The pool is almost full (>90%). New connections might be rejected soon.",
                                "manual_close": "YES"
                            }
                        ]
                    },
                    {
                        "uuid": "550f649268724107a3c41995a0e6d7d4",
                        "name": "Get All Metrics",
                        "type": "HTTP_AGENT",
                        "key": "keycloak.get_metrics",
                        "delay": "5m",
                        "history": "7d",
                        "value_type": "TEXT",
                        "description": "bin/kc.[sh|bat] start --metrics-enabled=true",
                        "preprocessing": [
                            {
                                "type": "JAVASCRIPT",
                                "parameters": [
                                    "// Replace all instances of \\\" with a single quote ' to fix Zabbix parsing error\nreturn value.replace(/\\\\\"/g, \"'\");"
                                ]
                            }
                        ],
                        "url": "{$KEYCLOAK.METRICS.URL}",
                        "triggers": [
                            {
                                "uuid": "3cdeb3ae42bc42d39239efea9ee45c7b",
                                "expression": "nodata(/Template App Keycloak by HTTP/keycloak.get_metrics,5m)=1",
                                "name": "Failed to fetch metrics (Endpoint unreachable)",
                                "priority": "DISASTER",
                                "description": "Fires if no data is received for 5 minutes.",
                                "manual_close": "YES"
                            }
                        ]
                    },
                    {
                        "uuid": "aa408da85e194316ba8595aa90bfee12",
                        "name": "Keycloak Health Status",
                        "type": "HTTP_AGENT",
                        "key": "keycloak.health.status",
                        "delay": "5m",
                        "value_type": "CHAR",
                        "description": "Check if an instance has finished its start up and is ready to serve requests by calling its health REST endpoints.",
                        "preprocessing": [
                            {
                                "type": "JSONPATH",
                                "parameters": [
                                    "$.body.status"
                                ]
                            }
                        ],
                        "url": "{$KEYCLOAK.HEALTH.URL}",
                        "headers": [
                            {
                                "name": "Accept",
                                "value": "application/json"
                            }
                        ],
                        "output_format": "JSON",
                        "tags": [
                            {
                                "tag": "component",
                                "value": "health"
                            }
                        ],
                        "triggers": [
                            {
                                "uuid": "9fd9d6f1d41d4d94aa277a1238686e28",
                                "expression": "last(/Template App Keycloak by HTTP/keycloak.health.status)<>\"UP\" or nodata(/Template App Keycloak by HTTP/keycloak.health.status, 3m)=1",
                                "name": "Keycloak Health Check is DOWN",
                                "opdata": "Value: {ITEM.LASTVALUE}",
                                "priority": "DISASTER",
                                "description": "Alerts immediately if the health check endpoint returns anything other than \"UP\".",
                                "manual_close": "YES"
                            }
                        ]
                    },
                    {
                        "uuid": "ea5e1b1a2a6a4c95aed7bfaa2531490c",
