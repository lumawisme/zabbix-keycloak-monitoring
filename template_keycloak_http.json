                    {
                        "uuid": "ca0ffdec41ba451fbf3668e033e3e502",
                        "name": "Login Errors Rate",
                        "type": "DEPENDENT",
                        "key": "keycloak.login.errors.rate",
                        "value_type": "FLOAT",
